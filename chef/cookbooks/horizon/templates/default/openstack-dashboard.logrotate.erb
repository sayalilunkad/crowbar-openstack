/var/log/apache2/openstack-dashboard-access_log {
    compress
    copytruncate
    dateext
    maxage 365
    rotate 99
    size=+4096k
    notifempty
    missingok
}

/var/log/apache2/openstack-dashboard-error_log {
    compress
    copytruncate
    dateext
    maxage 365
    rotate 99
    size=+1024k
    notifempty
    missingok
}
